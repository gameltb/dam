{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w package.json --exec \"bash -c \\\"npm run dev:concurrently\\\"\"",
    "dev:concurrently": "concurrently \"npm run client:dev -- --host\" \"npm run spacetime:start\" \"npm run spacetime:dev\" \"npm run server:debug\"",
    "client:dev": "vite",
    "spacetime:start": "spacetime start --pg-port 5432 --data-dir /tmp/spacetime/data",
    "spacetime:dev": "spacetime dev -s local --database flowcraft --module-project-path spacetime-module --module-bindings-path src/generated/spacetime",
    "spacetime:publish": "spacetime publish -s local --delete-data --break-clients --project-path spacetime-module -y flowcraft",
    "spacetime:generate": "spacetime generate --lang typescript --out-dir src/generated/spacetime --project-path spacetime-module",
    "server:dev": "tsx watch src/server/index.ts",
    "server:debug": "tsx --inspect=0.0.0.0:9229 src/server/index.ts",
    "build": "npm run type-check && vite build",
    "dev:build": "eslint . --config eslint.dev.config.js --fix; npm run build",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "format": "prettier --write . --log-level warn",
    "type-check": "tsc --build",
    "knip": "knip",
    "test": "vitest run",
    "verify": "set -ex; npm run lint:fix; npm run test; npm run build; npm run format",
    "preview": "vite preview",
    "proto:generate": "rm -rf src/generated/flowcraft src/generated/schemas && npx buf generate && npx buf generate --template buf.gen.schema.yaml",
    "prepare": "husky"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.2",
    "@connectrpc/connect": "^2.1.1",
    "@connectrpc/connect-web": "^2.1.1",
    "@fastify/multipart": "^9.3.0",
    "@fastify/static": "^9.0.0",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@rjsf/core": "^6.1.2",
    "@rjsf/utils": "^6.1.2",
    "@rjsf/validator-ajv8": "^6.1.2",
    "@tailwindcss/typography": "^0.5.19",
    "@xyflow/react": "^12.10.0",
    "ai": "^6.0.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dagre": "^0.8.5",
    "dotenv": "^17.2.3",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.562.0",
    "motion": "^12.24.0",
    "nanoid": "^5.1.6",
    "openai": "^6.15.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-markdown": "^10.1.0",
    "shiki": "^3.20.0",
    "spacetimedb": "^1.11.4",
    "streamdown": "^1.6.10",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tokenlens": "^1.3.1",
    "use-stick-to-bottom": "^1.1.1",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "zundo": "^2.3.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.62.1",
    "@bufbuild/protoc-gen-es": "^2.10.2",
    "@connectrpc/connect-fastify": "^2.1.1",
    "@connectrpc/connect-node": "^2.1.1",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/dagre": "^0.7.53",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/winston": "^2.4.4",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^5.3.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-unused-imports": "^4.3.0",
    "fastify": "^5.6.2",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "knip": "^5.79.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
